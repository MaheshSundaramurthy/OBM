[obm-release]
name=obm-release
baseurl=http://rpm.obm.org/32/release/$releasever
enabled=0
gpgcheck=0

[obm-contrib]
name=obm-contrib
baseurl=http://rpm.obm.org/32/contrib/$releasever
skip_if_unavailable=1
enabled=1
gpgcheck=0

[obm-stable]
name=obm-stable
baseurl=http://rpm.obm.org/32/stable/latest/$releasever
enabled=1
gpgcheck=0

[obm-master]
name=obm-master
baseurl=http://rpm.obm.org/32/master/latest/$releasever
enabled=0
gpgcheck=0
